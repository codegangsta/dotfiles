{
  "window.commandCenter": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.colorTheme": "Nord",
  "editor.fontSize": 13,
  "vscode-neovim.compositeKeys": {},
  "editor.fontFamily": "MonaspiceNe Nerd Font Mono",
  "workbench.fontAliasing": "antialiased",
  "cursor.aipreview.enabled": true,
  "cursor.chat.smoothStreaming": true,
  "breadcrumbs.symbolPath": "off",
  "workbench.activityBar.orientation": "vertical",
  "docker.containers.groupBy": "Compose Project Name",
  "diffEditor.renderSideBySide": false,
  "git.closeDiffOnOperation": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "terminal.integrated.fontSize": 13,
  "workbench.activityBar.location": "top",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.codeActionsOnSave.rules": null,
  "[typescript]": {},
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.showTabs": "single",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 1,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "zenMode.showTabs": "none",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.occurrencesHighlight": "off",
  "editor.renderLineHighlight": "none",
  "editor.selectionHighlight": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": false,
  "telemetry.telemetryLevel": "off",
  "editor.wordWrap": "on",
  "terminal.integrated.tabs.showActions": "never",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.tabs.hideCondition": "singleGroup"
}
