{
  "window.commandCenter": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.colorTheme": "Nord",
  "editor.fontSize": 13,
  "vscode-neovim.compositeKeys": {},
  "editor.fontFamily": "SFMono Nerd Font",
  "workbench.fontAliasing": "antialiased",
  "cursor.aipreview.enabled": true,
  "cursor.chat.smoothStreaming": true,
  "breadcrumbs.symbolPath": "off",
  "workbench.activityBar.orientation": "vertical",
  "docker.containers.groupBy": "Compose Project Name",
  "diffEditor.renderSideBySide": false,
  "git.closeDiffOnOperation": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "terminal.integrated.fontSize": 13,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.codeActionsOnSave.rules": null,
  "[typescript]": {},
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "zenMode.showTabs": "none",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.occurrencesHighlight": "off",
  "editor.renderLineHighlight": "none",
  "editor.selectionHighlight": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.highlightActiveIndentation": false,
  "telemetry.telemetryLevel": "off",
  "editor.wordWrap": "on",
  "terminal.integrated.tabs.showActions": "never",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "editor.folding": false,
  "editor.showFoldingControls": "never",
  "editor.guides.indentation": false,
  "editor.cursorStyle": "block",
  // "scm.diffDecorations": "none",
  "git.autofetch": true,
  "cursor.general.enableShadowWorkspace": true,
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "git.branchProtection": [
    "develop",
    "master"
  ],
  "sqltools.results.reuseTabs": "connection",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
